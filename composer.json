{
    "name": "xiaohuilam/laravel-time-pattern",
    "description": "Laravel 匹配日期时间",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@aliyun.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.3",
        "laravel/framework": ">=5.0",
        "fukuball/jieba-php": "^0.33.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<8.0",
        "laravel/laravel": ">=5.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "<=v4.2.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Xiaohuilam\\LaravelTimePattern\\": "src/"
        },
